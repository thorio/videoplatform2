generator client {
    provider = "prisma-client-js"
    output   = "../.prisma"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id           Int     @id @default(autoincrement())
    username     String  @unique
    passwordHash String
    videos       Video[]
}

model Video {
    id         Int    @id @default(autoincrement())
    title      String
    tags       String
    uploaderId Int
    uploader   User   @relation(fields: [uploaderId], references: [id])
}
